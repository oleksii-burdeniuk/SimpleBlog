{
  "expo": {
    "name": "SimpleBlog",
    "slug": "SimpleBlog",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Sebastien.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a7ea4"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ic_launcher-66e4a576499b5/ios/iTunesArtwork@1x.png",
        "backgroundColor": "#0a7ea4"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images//ic_launcher-66e4a576499b5/android/mipmap-xxxhdpi/ic_launcher_foreground.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
